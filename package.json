    "url": "git+https://github.com/qntfrd/ezmqp.git"
  },
  "bugs": {
    "url": "https://github.com/qtfrd/ezmqp/issues"
  },
  "devDependencies": {
    "@types/amqplib": "0.10.4",
    "@types/chai": "5.0.0",
    "@types/mocha": "10.0.6",
    "@types/sinon": "17.0.2",
    "chai": "5.1.1",
    "mocha": "10.2.0",
    "nyc": "15.1.0",
    "sinon": "17.0.1",
    "ts-node": "10.9.1",
    "typedoc": "0.23.14",
    "typescript": "4.8.3"
  },
  "dependencies": {
